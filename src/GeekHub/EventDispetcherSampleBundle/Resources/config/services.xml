<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <parameters>
        <parameter key="geek_hub_event_dispetcher_sample.example.class">GeekHub\EventDispetcherSampleBundle\Example
        </parameter>
    </parameters>

    <services>
        <service id="sample1" class="GeekHub\EventDispetcherSampleBundle\Service\MySample1">
            <tag name="kernel.event_listener" event="my_sample_event" method="onHandle" priority="0"/>
        </service>
        <service id="sample2" class="GeekHub\EventDispetcherSampleBundle\Service\MySample2">
            <tag name="kernel.event_listener" event="my_sample_event" method="onHandle" priority="1"/>
        </service>
    </services>

</container>
